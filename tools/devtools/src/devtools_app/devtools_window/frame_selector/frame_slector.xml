<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
  <t t-name="devtools.FrameSelector" owl="1">
    <select t-if="store.frameUrls.length > 1" class="form-select form-select-sm custom-select navbar-select" t-on-change="selectFrame">
      <t t-foreach="store.frameUrls" t-as="frame" t-key="frame_index">
        <option t-att-value="frame"><t t-out="frame"/></option>
      </t>
    </select>
  </t>
</templates>